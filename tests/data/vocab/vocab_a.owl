<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/owl#"
     xml:base="http://example.org/owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://example.org/owl"/>

    <!-- Define a class -->
    <owl:Class rdf:about="http://example.org/owl#Animal">
        <rdfs:label>Animal</rdfs:label>
        <rdfs:comment>An example class for animals.</rdfs:comment>
    </owl:Class>

    <!-- Define an individual -->
    <owl:NamedIndividual rdf:about="http://example.org/owl#Dog">
        <rdf:type rdf:resource="http://example.org/owl#Animal"/>
        <rdfs:label>Dog</rdfs:label>
        <rdfs:comment>A specific example of an animal.</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
